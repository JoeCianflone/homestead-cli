#!/usr/bin/env php
<?php
require __DIR__.'/vendor/autoload.php';
require __DIR__.'/src/Core/bootstrap.php';

use App\Core\Container;
use App\Commands\FreshCommand;
use App\Commands\NewSiteCommand;
